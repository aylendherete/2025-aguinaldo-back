meta {
  name: Cancel Payment Register Forbidden
  type: http
  seq: 20
}

patch {
  url: {{baseUrl}}/api/payments/turn/{{turnId}}/cancel
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

assert {
  res.status: eq 403
}

tests {
  test("Should prevent non-doctor/non-admin from canceling payment", function() {
    expect(res.getStatus()).to.equal(403);
  });
}
